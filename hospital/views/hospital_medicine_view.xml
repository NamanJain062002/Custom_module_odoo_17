<odoo>
    <record model="ir.ui.view" id="hospital_hospiatal_paitent_medicine_form_view_id">
        <field name="name">hospiatal.paitent.medicine.form.view.id</field>
        <field name="model">hospiatal.paitent.medicine</field>
        <field name="arch" type="xml">
            <form string="medicine Form">

                <group>
                    <field name="name" invisible="1"/>
                    <field name="med_name"/>
                    <field name="qty"/>
                </group>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="hospital_hospiatal_paitent_medicine_tree_view_id">
        <field name="name">hospiatal.paitent.medicine.tree.view.id</field>
        <field name="model">hospiatal.paitent.medicine</field>
        <field name="arch" type="xml">
            <tree string="Hospital Tree">
               <field name="name" invisible="1"/>
                <field name="med_name"/>
            </tree>
        </field>
    </record>

</odoo>