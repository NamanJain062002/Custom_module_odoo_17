<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="js_practice.ListButtons" t-inherit="hr_expense.ListButtons"  t-inherit-mode="extension">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button class="btn btn-secondary" t-on-click="add_btn">
                CreateReport Dummy</button>
        </xpath>
    </t>

      <t t-name="js_practice.expensebtn" t-inherit="web_gantt.GanttView.Buttons"  t-inherit-mode="extension">
        <xpath expr="//div[hasclass('o_gantt_buttons_container')]" position="after">
            <button class="btn btn-secondary" t-on-click="hi_method">
                TIME
            </button>
        </xpath>
    </t>

</templates>
